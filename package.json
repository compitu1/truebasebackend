  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^5.2.17",
    "node-fetch": "^2.2.0",
    "nodemailer": "^4.6.8",
    "random-number": "0.0.9",
    "react-mdl": "^1.11.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0"
  }
}
